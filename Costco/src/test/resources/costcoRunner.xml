<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Amazon Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="firefox"/>
    <parameter name="browserVersion" value="72"/>
    <parameter name="url" value="https://www.costco.com"/>

    <test name = "Costco Home">
        <classes>
            <class name="testSearch.TestSearch"/>
            <class name="testSignUp.TestSignUp"/>
            <class name="testSignIn.TestSignIn"/>
            <class name="testShopAllDepartements.TestShopAllDepartments"/>
            <class name="testTravel.TestTravel"/>
            <class name="testDatasearch.TestDataSearch"/>
            <class name="testkeyword.TestKeyword"/>
            <class name="testPhoto.TestPhoto"/>
            <class name="testMyAccount.TestMyAccount"/>
            <class name="testService.TestService"/>



        </classes>
    </test>

</suite>
